top :15px;\n  color: $white_color;\n  font-size: 20px;\n}\n\n.form{\n  @include flex_column();\n  width: 100%;\n}\n.form__element{\n  margin: auto;\n  margin-bottom: 1em;\n  border-radius: 10px;\n  padding-left: 10px;\n  height: 40px;\n}\n.form__textarea{\n  height : 100px !important;\n  padding-top: 10px;\n}\n.order-button{\n  // display: flex;\n  // align-items: center;\n  text-align: center;\n  width: 200px;\n  margin: auto;\n  border-radius: 10px;\n  height: 30px;\n  color: rgba(0, 0, 0);\n  :hover{\n    background-color: $white_color !important;\n  }\n}\n.loading-gif{\n  width: 30px;\n  height: 30px;\n}\n.modify-icon-button{\n  font-size: 25px !important;\n  color: $primary_color;\n}\n@media screen and (min-width:1000px){\n  .form__element{\n    width : 50%;\n  }\n}\n@media screen and (max-width : 999px){\n  .form__element{\n    width: 70%;\n  }\n}\n@media screen and (max-width:600px){\n  .form__element{\n    width: 90%;\n  }\n}\n@media screen and (max-width:230px){\n  .order-button{\n    width : 90%;\n  }\n}\n\n.main--modified-heading {\n  margin-bottom: 10px;\n}\n\n.shop-nav {\n  justify-content: space-between;\n  align-items: center;\n  background-image: linear-gradient(to bottom right, #693628, #582b17);\n  height: 120px;\n  padding: 0 50px 0 50px;\n  width: 100%;\n}\n\n.shop-nav__image {\n  width: auto;\n}\n\n.button--like {\n  color: black;\n}\n\n.shop-nav__link {\n  font-weight: bolder;\n  padding: 0.5em;\n  justify-content: center;\n  width: 200px;\n}\n\n@media screen and (max-width: 600px) {\n  .shop-nav {\n    height: 200px;\n    flex-wrap: wrap;\n  }\n\n  .shop-nav__link {\n    width: 300px;\n  }\n}\n@media screen and (max-width: 400px) {\n  .shop-nav__image {\n    width: 100%;\n    object-fit: contain;\n  }\n\n  .shop-nav__link {\n    width: 100%;\n  }\n}\n\n.main {\n  padding: 2em inherit 2em inherit;\n  height: auto;\n  position: relative;\n}\n.main__link-navigator-trigger {\n  position: absolute;\n  top: 12px;\n  height: min-content;\n  font-size: 20px;\n  color: $primary_color;\n  left: 40px;\n  display: none;\n}\n.main__link-navigator-trigger-close {\n  color: $white_color;\n  position: absolute;\n  right: 20px;\n  top: 25px;\n  font-size: 20px;\n  display: none;\n}\n.main__span {\n  transition-duration: 1s;\n  display: block;\n  width: 60px;\n  height: 5px;\n  background-color: #2a0e02;\n  border-radius: 10px;\n}\n\n.unordered-list {\n  list-style-type: none;\n  padding: 0;\n  width: 100%;\n}\n\n.item-list {\n  display: flex;\n  flex-direction: row;\n  width: 100%;\n  border-bottom: solid rgba(197, 194, 194, 0.9) thin;\n  color: black;\n  font-weight: bolder;\n}\n\n.category__button {\n  width: 100%;\n  height: 100%;\n  background: inherit;\n  text-align: left;\n  padding: 10px 0 10px 10px;\n  transition-duration: 0.4s;\n  outline: none;\n  font-size: 16px;\n  text-transform: capitalize;\n}\n\n.category__button:hover {\n  background-color: #2a0e02;\n  color: white;\n  outline: none;\n}\n\n.category-all__header {\n  justify-content: space-between;\n}\n\n.category-all__header-form {\n  align-items: center;\n}\n\nbutton {\n  outline: none;\n  border: none;\n}\n\nbutton:hover {\n  cursor: pointer;\n}\n\n.button-reset {\n  background-color: #2a0e02;\n  outline: none;\n  border: none;\n  color: #ffffff;\n  font-weight: bolder;\n  height: min-content;\n  padding: 7px 30px 7px 30px;\n  margin-right: 10px;\n  border-radius: 10px;\n}\n\n.icon-search {\n  background-color: transparent;\n}\n\n.category-all__search-field {\n  display: none;\n}\n\n.search-form {\n  display: flex;\n  flex-direction: row;\n  justify-content: flex-end;\n  width: 100%;\n}\n\n.search-input {\n  border-radius: 10px;\n  border: solid rgba(197, 194, 194, 0.9) thin;\n  outline: none;\n  padding: 5px 10px;\n  width: 50%;\n  color: black;\n  font-weight: bolder;\n}\n\n.category-all__item-collection {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-around;\n  padding: 20px 0 20px 0;\n  flex-wrap: wrap;\n  -ms-flex-wrap: wrap;\n}\n\n.category-all__each-item {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  border: solid rgba(197, 194, 194, 0.9) thin;\n  transition-duration: 1s;\n  position: relative;\n  margin-bottom: 1.5em;\n  z-index: 99;\n}\n\n.category-all__each-item:hover .send-message {\n  display: flex;\n}\n\n.category-all__each-item--hover:hover {\n  background-color: rgba(0, 0, 0, 0.1);\n}\n\n.category-all__each-item-image-div {\n  width: 100%;\n  height: min-content;\n  display: flex;\n  flex-direction: row;\n  justify-content: center;\n}\n\n.category-all__each-item-image {\n  width: 50%;\n  margin: auto;\n  object-fit: contain;\n}\n\n.category-all__each-item-text-container {\n  padding: 20px 15px 20px 15px;\n}\n\n.category-all__each-item-text-container * {\n  text-transform: uppercase;\n}\n\n.category-all__each-item-text {\n  font-weight: bolder;\n  font-size: 14px;\n}\n\n.category-all__each-label {\n  color: rgba(73, 0, 0, 0.671);\n  font-weight: bold;\n}\n\n.send-message {\n  display: flex;\n  flex-direction: row;\n  justify-content: center;\n  align-items: center;\n  position: absolute;\n  bottom: 0px;\n  width: 100%;\n  @include animation(messageslide, 0.4s, ease-in-out);\n  background-color: rgba(42, 14, 2, 0.9);\n  display: none;\n  height: 100%;\n}\n\n.message-button {\n  width: min-content;\n  background-color: unset;\n  color: #ffffff;\n}\n\n@keyframes messageslide {\n  from {\n    height: 50%;\n  }\n  to {\n    height: 100%;\n  }\n}\n@keyframes slideleft {\n  from {\n    left: -100px;\n  }\n  to {\n    left: 0 px;\n  }\n}\n@keyframes slideback {\n  from {\n    left: 0px;\n  }\n  to {\n    left: -300px;\n    // font-size: 100px;\n  }\n}\n@media screen and (min-width: 800px) {\n  .main {\n    padding: 2em 50px 2em 50px;\n    justify-content: space-between;\n  }\n\n  .main__category {\n    width: 30%;\n  }\n\n  .main__category-all {\n    width: 65%;\n  }\n}\n@media screen and (min-width: 1000px) {\n  .category-all__each-item {\n    width: 30%;\n  }\n\n  .main__category {\n    width: 25%;\n  }\n\n  .main__category-all {\n    width: 70%;\n  }\n}\n@media screen and (max-width: 999px) {\n  .category-all__each-item {\n    width: 45%;\n    margin-bottom: 2em;\n  }\n}\n@media screen and (max-width: 800px) {\n  .main {\n    padding: 2em 10px 2em 10px;\n    justify-content: space-between;\n  }\n\n  .main__category {\n    width: 25%;\n  }\n\n  .main__category-all {\n    width: 70%;\n  }\n}\n@media screen and (max-width: 600px) {\n  .main__link-navigator-trigger {\n    display: block;\n  }\n  .main__link-navigator-trigger-close {\n    display: block;\n  }\n  .main__category {\n    display: none;\n    position: absolute;\n    left: 0;\n    top: 0;\n    width: 60%;\n    z-index: 999;\n    background-color: #582b17;\n    color: $white_color;\n    padding-left: 20px;\n    padding-right: 20px;\n    @include animation(slideleft, 0.4s, ease-in-out);\n    transition: all ease-in-out 4s;\n  }\n  .slideback {\n    @include animation(slideback, 0.4s, ease-in-out);\n    transition: all ease-in-out 4s;\n  }\n  .category-all__each-item {\n    width: 90%;\n  }\n  .category__button {\n    color: $white_color;\n  }\n  .main__category-all {\n    width: 90%;\n    margin: auto;\n  }\n}\n@media screen and (max-width: 360px) {\n  .main__category {\n    width: 100%;\n  }\n}\n/*# sourceMappingURL=shop.scss.map */\n"},"lineCount":null}},"error":null,"hash":"cc1801c4972ca6b01bcea0f9084d0a60","cacheData":{"env":{}}}
